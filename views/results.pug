
doctype html
html(lang='en')
  head
    meta(charset='UTF-8')
    meta(name='viewport', content='width=device-width, initial-scale=1.0')
    title The SyllySyncer
    link(rel='stylesheet', href='style.css')
  body
    .container
      .top-banner
        .left-section
          span The SyllySyncer
        .right-section
          a(href='/options') Home     
          a(href='/') Login
    .content-container
      .scrollable-box
            each student in matchingStudents
              each temp in student.timeArray
                p= temp
              
           
    .result-button-container 
        a.button-left(href='/options') Home
        a.button-right(href='#') Export